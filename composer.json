{
    "name": "laravel/vapor-core",
    "description": "The kernel and invocation handlers for Laravel Vapor",
    "keywords": [
        "laravel",
        "vapor"
    ],
    "homepage": "https://github.com/laravel/vapor-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "aws/aws-sdk-php": "^3.80",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "hollodotme/fast-cgi-client": "^3.0",
        "illuminate/container": "^8.0|^9.0|^10.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/queue": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "monolog/monolog": "^2.0|^3.2",
        "nyholm/psr7": "^1.0",
        "riverline/multipart-parser": "^2.0.9",
        "symfony/process": "^5.0|^6.0",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "require-dev": {
        "laravel/octane": "^1.5|^2.3",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Vapor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Vapor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Vapor\\VaporServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
